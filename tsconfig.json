{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["assets/*"],
      "@constants/*": ["src/constants/*"],
      "@signals/*": ["src/signals/*"],
      "@api/*": ["src/api/*"],
    }
  }
}
